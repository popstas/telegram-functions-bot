{
  "name": "telegram-functions-bot",
  "version": "main",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "changelog": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s -r 0",
    "test": "node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "npm test -- --coverage",
    "test-full": "npm run test && npm run typecheck && npm run lint src tests",
    "prepare-commit": "npm run test-full && npm run format",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier -l --write \"src/**/*.{ts,js,json,md}\" \"tests/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\" \"tests/**/*.{ts,js,json,md}\"",
    "agent": "tsx src/cli-agent.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@agentic/core": "^7.1.0",
    "@modelcontextprotocol/sdk": "^1.11.4",
    "express": "^4.21.1",
    "form-data": "^4.0.3",
    "google-auth-library": "^9.14.2",
    "googleapis": "^144.0.0",
    "https-proxy-agent": "^7.0.2",
    "js-tiktoken": "^1.0.15",
    "js-yaml": "^4.1.0",
    "langfuse": "^3.37.2",
    "lodash.debounce": "^4.0.8",
    "openai": "^4.67.2",
    "recursive-readdir": "^2.2.3",
    "telegraf": "^4.15.0",
    "telegramify-markdown": "^1.3.0",
    "tmp": "^0.2.3",
    "tsx": "^4.9.3",
    "zod": "^3.23.8",
    "mqtt": "^5.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@jest/globals": "^30.0.0-beta.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^22.7.5",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.0",
    "eslint": "^9.25.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "overrides": {
    "esbuild": "^0.24.0"
  }
}
